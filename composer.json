{
    "name": "koalafacade/filament-crash",
    "description": "Filament plugin to enforce you client paid your filament project",
    "type": "library",
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.16"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "laravel/pint": "^1.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "KoalaFacade\\FilamentCrash\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Stiven Trizky Katuuk",
            "email": "pemudakoding@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "KoalaFacade\\FilamentCrash\\FilamentCrashServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
